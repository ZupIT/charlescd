{
  "name": "darwin-deploy",
  "version": "0.1.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node --files -r tsconfig-paths/register src/main.ts",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
    "start:prod": "node dist/main.js",
    "lint": "eslint --max-warnings 0 . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --fix --max-warnings 0 . --ext .js,.jsx,.ts,.tsx",
    "test": "jest --testRegex \"\\.spec\\.ts$\" ",
    "test:integration": "jest --testRegex \"integration-spec\\.ts$\" --runInBand --forceExit",
    "test:cov": "jest \"\\.spec.ts$\" --coverage",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "test:all:cov": "jest --runInBand --coverage --forceExit"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.4.1",
    "@kubernetes/client-node": "^0.14.0",
    "@nestjs/axios": "0.0.2",
    "@nestjs/common": "^8.1.1",
    "@nestjs/config": "^1.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/mapped-types": "^1.0.0",
    "@nestjs/microservices": "^8.0.0",
    "@nestjs/platform-express": "^8.1.1",
    "@nestjs/platform-socket.io": "^8.0.0",
    "@nestjs/schedule": "^1.0.1",
    "@nestjs/typeorm": "^8.0.2",
    "@nestjs/websockets": "^8.0.0",
    "@octokit/rest": "^16.43.1",
    "@types/lru-cache": "^5.1.0",
    "@types/rimraf": "^3.0.0",
    "@types/stack-trace": "0.0.29",
    "amqp-connection-manager": "^3.7.0",
    "amqplib": "^0.8.0",
    "cache-manager": "^3.4.4",
    "class-transformer": "^0.3.1",
    "cls-hooked": "^4.2.2",
    "cls-rtracer": "^1.4.1",
    "consul": "^0.36.0",
    "fastify-swagger": "^4.12.4",
    "joi": "^17.4.0",
    "js-yaml": "^3.13.1",
    "kafkajs": "^1.15.0",
    "lodash": "^4.17.21",
    "lru-cache": "^6.0.0",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "mqtt": "^4.2.8",
    "mustache": "^3.2.1",
    "nats": "^2.2.0",
    "openpgp": "^5.0.0-1",
    "pg": "^7.18.2",
    "pg-boss": "^5.0.1",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "stack-trace": "0.0.10",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.34",
    "uuid": "8.3.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/types": "7.5.5",
    "@nestjs/swagger": "^5.1.0",
    "@nestjs/testing": "^8.1.1",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.16.1",
    "@types/jest": "^27.0.0",
    "@types/lodash": "^4.14.149",
    "@types/morgan": "^1.9.0",
    "@types/node": "11.13.4",
    "@types/supertest": "2.0.7",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^4.11.1",
    "eslint": "^7.1.0",
    "hpropagate": "0.0.6",
    "jest": "^27.2.5",
    "jest-html-reporters": "^2.0.1",
    "node-notifier": "^8.0.1",
    "prettier": "^1.19.1",
    "rxjs": "^7.2.0",
    "supertest": "4.0.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^9.0.0",
    "tsc-watch": "2.2.1",
    "tsconfig-paths": "3.8.0",
    "typescript": "^4.2.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "app/**/*.ts",
      "!app/**/resources/**",
      "!app/**/{index.ts,*.{module,enum,interface,constants,dto,repository,schema}.ts}"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "reporters": [
      "default",
      [
        "jest-html-reporters",
        {
          "publicPath": "./coverage/lcov-report",
          "filename": "report.html",
          "expand": true
        }
      ]
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "testTimeout": 10000
  }
}
