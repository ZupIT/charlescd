{
  "name": "ui",
  "version": "2.0.0",
  "private": true,
  "description": "Charles C.D. | Fastest hypothesis validation with Circle Deployment",
  "homepage": "https://github.com/ZupIT/darwin-ui#README",
  "bugs": {
    "url": "https://github.com/ZupIT/darwin-ui/issues"
  },
  "author": "ZUP Innovation",
  "scripts": {
    "build": "react-scripts build",
    "start": "cross-env ENVIRONMENT=development node env-writer.js path=./public && cross-env BROWSER=none react-scripts start",
    "start:local": "cross-env ENVIRONMENT=local node env-writer.js path=./public && cross-env BROWSER=none react-scripts start",
    "test": "react-scripts test --watchAll=false --testPathIgnorePatterns='fixtures.ts'",
    "test:coverage": "react-scripts test --watchAll=false --coverage --testPathIgnorePatterns='storybook.test.js|fixtures.ts'",
    "test:watch": "react-scripts test --testPathIgnorePatterns='fixtures.ts'",
    "test:storybook": "react-scripts test --testPathIgnorePatterns='setupTests'",
    "storybook": "start-storybook -p 6006 -s public --ci",
    "build:storybook": "build-storybook -s public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "import/no-anonymous-default-export": "off"
    },
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,ts,tsx}"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/unit-test/",
      "/src/core/i18n/",
      "/src/index.tsx",
      "/stub/"
    ],
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 26,
        "functions": 27,
        "lines": 36,
        "statements": 36
      }
    }
  },
  "dependencies": {
    "@casl/ability": "4.0.7",
    "@casl/react": "2.1.0",
    "@popperjs/core": "2.9.2",
    "apexcharts": "3.27.2",
    "cross-env": "7.0.3",
    "dayjs": "1.10.6",
    "debounce-promise": "3.1.2",
    "jwt-decode": "3.1.2",
    "lodash": "4.17.21",
    "prism-react-renderer": "1.2.1",
    "react": "17.0.2",
    "react-apexcharts": "1.3.9",
    "react-content-loader": "5.1.4",
    "react-cookies": "0.1.1",
    "react-dom": "17.0.2",
    "react-hook-form": "6.15.8",
    "react-popper": "2.2.5",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "react-select": "3.2.0",
    "react-svg": "11.2.5",
    "react-tooltip": "4.2.21",
    "styled-components": "5.3.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "6.3.4",
    "@storybook/addon-essentials": "6.3.4",
    "@storybook/addon-links": "6.3.4",
    "@storybook/addon-storyshots": "6.3.4",
    "@storybook/node-logger": "6.3.4",
    "@storybook/preset-create-react-app": "3.2.0",
    "@storybook/react": "6.3.4",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "11.2.7",
    "@testing-library/react-hooks": "5.1.3",
    "@testing-library/user-event": "12.8.3",
    "@types/debounce-promise": "3.1.4",
    "@types/lodash": "4.14.171",
    "@types/node": "12.20.16",
    "@types/react": "17.0.14",
    "@types/react-beautiful-dnd": "12.1.4",
    "@types/react-cookies": "0.1.0",
    "@types/react-dom": "17.0.9",
    "@types/react-router-dom": "5.1.8",
    "@types/react-select": "3.1.2",
    "@types/styled-components": "5.1.11",
    "jest-fetch-mock": "3.0.3",
    "jest-styled-components": "7.0.5",
    "msw": "0.27.2",
    "mutationobserver-shim": "0.3.7",
    "react-intersection-observer": "8.32.0",
    "react-select-event": "4.2.0",
    "typescript": "4.3.5"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
